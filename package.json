{
  "name": "pharmacy-management-system",
  "version": "1.0.0",
  "description": "Full Pharmacy Management System built with React and Node.js using Argon Dashboard.",
  "main": "index.js",
  "proxy": "http://localhost:5000",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify:scss": "sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --style compressed",
    "build:scss": "npm run compile:scss && npm run minify:scss"
  },
  "keywords": [
    "pharmacy",
    "dashboard",
    "react",
    "nodejs",
    "mern",
    "medicine",
    "management"
  ],
  "author": "Your Name or Company",
  "license": "MIT",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.1",
    "@react-oauth/google": "^0.12.2",
    "ajv": "^8.11.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "classnames": "2.3.2",
    "concurrently": "^9.2.0",
    "jwt-decode": "^4.0.0",
    "moment": "2.29.4",
    "nouislider": "15.4.0",
    "react": "18.2.0",
    "react-barcode-reader": "^0.0.2",
    "react-chartjs-2": "^5.3.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-datetime": "3.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.21.1",
    "react-scripts": "^5.0.1",
    "react-toastify": "^11.0.5",
    "reactstrap": "^9.2.3",
    "sass": "1.69.5"
  },
  "devDependencies": {
    "gulp": "^5.0.1",
    "gulp-append-prepend": "1.0.9"
  },
  "optionalDependencies": {
    "eslint-plugin-flowtype": "8.0.3",
    "typescript": "5.3.3"
  }
}
